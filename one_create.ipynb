   "source": [
    "# N, K 입력받기\n",
    "n, k = map(int, input().split())\n",
    "\n",
    "count = 0\n",
    "while n != 1:\n",
    "  if(n % k == 0):\n",
    "    n //= k\n",
    "    count = count + 1\n",
    "  else:\n",
    "    n -= 1\n",
    "    count = count + 1\n",
    "\n",
    "print(count)"
   ]
